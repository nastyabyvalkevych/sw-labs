@prefix :       <http://example.org/sw#> .
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix foaf:   <http://xmlns.com/foaf/0.1/> .
@prefix schema: <https://schema.org/> .
@prefix dbo:    <http://dbpedia.org/ontology/> .

:Alloy      rdfs:label "Сплав"@uk .
:Brass      rdfs:label "Латунь"@uk .
:Copper     rdfs:label "Мідь"@uk .
:Zinc       rdfs:label "Цинк"@uk .

:SPIEGEL    rdfs:label "SPIEGEL" .
:Hamburg    rdfs:label "Гамбург"@uk .
:Germany    rdfs:label "Німеччина"@uk .

:Essay      rdfs:label "Есе"@uk .
:Intro      rdfs:label "Вступ"@uk .
:Body       rdfs:label "Основна частина"@uk .
:Conclusion rdfs:label "Висновок"@uk .

:Pavlo      rdfs:label "Павло"@uk .
:Olena      rdfs:label "Олена"@uk .
:Stefan     rdfs:label "Стефан"@uk .
:Anna       rdfs:label "Анна"@uk .
:Ivan       rdfs:label "Іван"@uk .

:Poltava    rdfs:label "Полтава"@uk .
:Kyiv       rdfs:label "Київ"@uk .
:Rome       rdfs:label "Рим"@uk .
:Italy      rdfs:label "Італія"@uk .


:hasComponent a rdf:Property ; rdfs:label "містить компонент"@uk .
:saysAbout   a rdf:Property ; rdfs:label "каже про"@uk .
:thinksAbout a rdf:Property ; rdfs:label "думає про"@uk .

# Латунь – це сплав міді та цинку
:Brass a :Alloy ;
  :hasComponent :Copper, :Zinc .

# SPIEGEL — німецький інформаційний журнал зі штаб-квартирою в Гамбурзі
:SPIEGEL a schema:Periodical ;
  schema:genre "Інформаційний журнал"@uk ;
  schema:inLanguage "de" ;
  dbo:headquarter :Hamburg ;

# Есе складається зі вступу, основної частини та висновку
:Essay schema:hasPart (
  :Intro
  :Body
  :Conclusion
) .

# Павло знає, що Олена живе в Полтаві
:stmt1 a rdf:Statement ;
  rdf:subject   :Olena ;
  rdf:predicate schema:homeLocation ;
  rdf:object    :Poltava .

:Pavlo schema:knowsAbout :stmt1 .

# Олена каже, що її друг живе в Києві
:FriendOfOlena foaf:knows :Olena .

:stmt2 a rdf:Statement ;
  rdf:subject   :FriendOfOlena ;
  rdf:predicate schema:homeLocation ;
  rdf:object    :Kyiv .

:Olena :saysAbout :stmt2 .

# Стефан думає, що Анна знає, що він знає її батька
:Anna schema:parent :AnnasFather .

:stmt3 a rdf:Statement ;
  rdf:subject   :Stefan ;
  rdf:predicate foaf:knows ;
  rdf:object    :AnnasFather .

:stmt4 a rdf:Statement ;
  rdf:subject   :Anna ;
  rdf:predicate foaf:knows ;
  rdf:object    :stmt3 .

:Stefan :thinksAbout :stmt4 .

# Іван знає, що Рим є столицею Італії
:stmt5 a rdf:Statement ;
  rdf:subject   :Italy ;
  rdf:predicate dbo:capital ;
  rdf:object    :Rome .

:Ivan schema:knowsAbout :stmt5 .
